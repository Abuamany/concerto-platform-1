<script type="text/ng-template" id="type_5_setter.html">
    {% embed "ConcertoPanelBundle:TestWizard:type_base_setter.html.twig" %}
        {% trans_default_domain "TestWizard" %}
        {% block setter %}
            <select ng-model="output" ng-change="onPrimitiveValueChange(output)" style="width:100%;"  ng-disabled="wizardObject.initProtected == '1'"
            ng-options="template.name as template.name for template in viewTemplateCollectionService.collection | filter : {'archived':'!1'} | orderBy: 'name'">
                <option value="">{{"none.choosen"|trans({},"panel")}}</option>
            </select>
        {% endblock %}
    {% endembed %}
</script>